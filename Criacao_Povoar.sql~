begin;
CREATE TABLE Partido(
	codigo_partido int, 
	nome_partido varchar(15) NOT NULL,
	PRIMARY KEY(codigo_partido));
CREATE TABLE Votacao(
	numero_votacao int, 
	data_votacao date NOT NULL, 
	descricao varchar(50) NOT NULL, 
	PRIMARY KEY(numero_votacao));
CREATE TABLE Urna(
	urna int,
	localidade varchar(15),
	PRIMARY KEY(urna));
CREATE TABLE Local_Votacao(
	num_votacao int, 
	urna int, 
	FOREIGN KEY(urna) REFERENCES Urna(urna), 
	FOREIGN KEY(num_votacao) REFERENCES Votacao(numero_votacao));
CREATE TABLE Eleitor(
	num_titulo int, 
	urna int, 
	nome varchar(15) NOT NULL, 
	data_nascimento date NOT NULL, 
	sexo char(1), 
	PRIMARY KEY(num_titulo), 
	FOREIGN KEY(urna) REFERENCES Urna(urna));
CREATE TABLE Candidato(
	num_titulo int, 
	cpf char(5) NOT NULL UNIQUE, 
	codigo_partido int, 
	num_candidato int, 
	PRIMARY KEY(num_candidato), 
	FOREIGN KEY(num_titulo) REFERENCES Eleitor(num_titulo), 
	FOREIGN KEY(codigo_partido) REFERENCES Partido(codigo_partido));
CREATE TABLE Fiscal(
	num_titulo int, 
	cpf char(5) NOT NULL UNIQUE,
	cpf_super char(5),
	FOREIGN KEY(num_titulo) REFERENCES Eleitor(num_titulo));
CREATE TABLE Votos(
	numero_votacao int,
	num_titulo int, 
	num_candidato int,
	FOREIGN KEY(numero_votacao) REFERENCES Votacao(numero_votacao),
	FOREIGN KEY(num_titulo) REFERENCES Eleitor(num_titulo),
	FOREIGN KEY(num_candidato) REFERENCES Candidato(num_candidato));
CREATE TABLE Telefone(
	num_titulo int,
	telefone1 varchar(11) NOT NULL,
	telefone2 varchar(11),
	telefone3 varchar(11),
	FOREIGN KEY(num_titulo) REFERENCES Eleitor(num_titulo));
CREATE TABLE Endereco(
	num_titulo int,
	rua varchar(20) NOT NULL,
	numero varchar(6) NOT NULL,
	complemento varchar(10),
	cidade varchar(20),
	uf varchar(2) NOT NULL,
	FOREIGN KEY(num_titulo) REFERENCES Eleitor(num_titulo));
CREATE TABLE Listagem(
	numero_votacao int,
	num_candidato int,
	FOREIGN KEY(numero_votacao) REFERENCES Votacao(numero_votacao),
	FOREIGN KEY(num_candidato) REFERENCES Candidato(num_candidato));
